{
  "name": "Soundcloud Stream Cleaner",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension removes all reposts from your Soundcloud Stream.",
  "homepage_url": "http://soundcloud.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "*://soundcloud.com/*"
      ],
      "js": [
        "src/inject/inject.js",
        "js/jquery/jquery.js"
      ]
    }
  ]
}